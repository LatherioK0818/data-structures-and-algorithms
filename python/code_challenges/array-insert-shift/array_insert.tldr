        "fill": "semi",
        "dash": "draw",
        "size": "xl",
        "font": "draw",
        "text": "def insertShiftArray(arr, value):\n    middle_index = (len(arr) + 1) // 2\n    new_array = [0] * (len(arr) + 1)\n\n    for i in range(middle_index):\n        new_array[i] = arr[i]\n\n    new_array[middle_index] = value\n\n    for i in range(middle_index, len(arr)):\n        new_array[i + 1] = arr[i]\n\n    return new_array",
        "align": "start",
        "verticalAlign": "middle",
